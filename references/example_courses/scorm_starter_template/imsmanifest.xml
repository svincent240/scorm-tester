        <item identifier="SCO-1" identifierref="RES-1" isvisible="true">
          <title>SCO 1</title>
          <!-- <metadata> element would go here -->
          <adlcp:dataFromLMS>SCO 1</adlcp:dataFromLMS>
          <!-- <adlcp:completionThreshold> element would go here -->
          <imsss:sequencing>
